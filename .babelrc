{
    "presets": [
        ["@babel/env", {"modules": false}],
        "@babel/react",
        "@emotion/babel-preset-css-prop"
    ],
    "plugins": [
        ["@babel/plugin-transform-runtime", {
            "regenerator": false
        }],
        "transform-object-rest-spread",
        "transform-class-properties",
    ],
    "env": {
        "test": {
            "presets": [
                "env",
                "react"
            ],
            "plugins": [
                "transform-object-rest-spread",
                "transform-class-properties",
                "add-react-displayname"
            ]
        }
    }
}
