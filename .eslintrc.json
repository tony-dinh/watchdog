{
    "extends": "react-app",
    "env": {
      "browser": true,
      "commonjs": true,
      "node": true,
      "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": ["emotion"],
    "rules": {
      "no-console": "off",
      "strict": ["error", "global"],
      "curly": "warn",
      "emotion/jsx-import": "error"
    }
  }
